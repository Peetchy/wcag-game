{
  "$schema": "https://json.schemastore.org/angularcli",
  "version": 1,
  "cli": {
    "analytics": false
  },
  "projects": {
    "a11y-wcag-game": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/a11y-wcag-game",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "a11y-wcag-game:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "a11y-wcag-game:build:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "a11y-wcag-game"
}

